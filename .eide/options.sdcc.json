{
    "version": 1,
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "cd \"${OutDir}\" && del *.asm *.rst *.sym *.cdb *.adb *._*"
        }
    ],
    "global": {
        "device": "mcs51",
        "optimize-type": "speed",
        "use-non-free": false,
        "specific-options": [
            "--model-small"
        ]
    },
    "c/cpp-compiler": {
        "language-c": "c99"
    },
    "asm-compiler": {},
    "linker": {
        "$mainFileName": "main"
    }
}